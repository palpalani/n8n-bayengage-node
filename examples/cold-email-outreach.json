          "value": "your-google-sheet-id"
        },
        "sheetName": {
          "__rl": true,
          "mode": "list",
          "value": "Leads"
        },
        "columnToMatchOn": "email",
        "valueToMatchOn": "={{ $json.email }}",
        "columns": {
          "mappingMode": "defineBelow",
          "value": {
            "status": "email_sent",
            "last_contact_date": "={{ $now.toISO() }}",
            "campaign_id": "={{ $json.campaignId || 'cold_outreach_' + $now.format('YYYY-MM-DD') }}"
          }
        }
      },
      "typeVersion": 4.5
    },
