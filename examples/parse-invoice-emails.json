          "id": "BAYENGAGE_API",
          "name": "BayEngage API"
        }
      }
    },
    {
      "parameters": {
        "resource": "event",
        "operation": "track",
        "eventName": "invoice_processed",
        "contactId": "={{ $('BayEngage Contact').item.json.id }}",
        "eventProperties": {
          "values": [
            {
              "name": "invoice_number",
              "value": "={{ $('RAG Agent').item.json.invoice_number || 'N/A' }}"
            },
            {
              "name": "amount",
              "value": "={{ $('RAG Agent').item.json.amount || '0' }}"
            },
            {
              "name": "status",
              "value": "={{ $('RAG Agent').item.json.payment_status || 'pending' }}"
            }
          ]
        }
      },
