      "typeVersion": 1,
      "position": [
        720,
        -40
      ]
    },
    {
      "parameters": {
        "resource": "contact",
        "operation": "upsert",
        "email": "={{ $json.from_email || $json.sender_email || 'vendor@example.com' }}",
        "firstName": "={{ $json.from_name || $json.sender_name || 'Vendor' }}",
        "lastName": "={{ $json.from_last_name || '' }}",
        "customFields": {
          "values": [
            {
              "name": "invoice_number",
              "value": "={{ $('RAG Agent').item.json.invoice_number || 'N/A' }}"
            },
            {
              "name": "invoice_amount",
              "value": "={{ $('RAG Agent').item.json.amount || '0' }}"
            },
            {
              "name": "due_date",
              "value": "={{ $('RAG Agent').item.json.due_date || 'N/A' }}"
            },
            {
              "name": "payment_status",
              "value": "={{ $('RAG Agent').item.json.payment_status || 'pending' }}"
            },
            {
              "name": "vendor_name",
              "value": "={{ $('RAG Agent').item.json.vendor || 'Unknown' }}"
            }
          ]
        }
      },
      "id": "bayengage-contact-upsert",
      "name": "BayEngage Contact",
      "type": "n8n-nodes-bayengage.bayEngage",
      "typeVersion": 1,
