      "parameters": {
        "promptType": "define",
        "text": "Handle data",
        "options": {
          "systemMessage": "You are an assistant for Parse Invoice Emails. Analyze invoice email content to extract key information including invoice number, amount, due date, vendor, payment status, and any special notes. Return a JSON object with structured invoice data and recommended follow-up actions."
        }
      },
      "id": "d2e31148-f0c1-4164-97f9-670bf929a2ce",
