            },
            {
              "name": "Payment Status",
              "value": "={{ $('RAG Agent').item.json.payment_status || 'pending' }}"
            },
            {
              "name": "Contact ID",
              "value": "={{ $('BayEngage Contact').item.json.id }}"
            },
            {
              "name": "Campaign ID",
              "value": "={{ $('BayEngage Create Campaign').item.json.id }}"
            }
          ]
        }
