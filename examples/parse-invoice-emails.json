    {
      "parameters": {
        "operation": "append",
        "documentId": {
          "__rl": true,
          "value": "SHEET_ID",
          "mode": "list",
          "cachedResultName": "Parse Invoice Emails"
        },
        "sheetName": {
          "__rl": true,
          "value": "Log",
          "mode": "list",
          "cachedResultName": "Log"
        },
        "columns": {
          "mappingMode": "defineBelow",
          "value": "Status,Invoice Number,Amount,Due Date,Vendor,Payment Status,Contact ID,Campaign ID",
          "schema": [
            {
              "name": "Status",
              "value": "={{ $json.success ? 'Processed' : 'Failed' }}"
            },
            {
              "name": "Invoice Number",
              "value": "={{ $('RAG Agent').item.json.invoice_number || 'N/A' }}"
