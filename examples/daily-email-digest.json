    },
    {
      "parameters": {
        "resource": "template",
        "operation": "create",
        "templateName": "Daily Digest - {{ new Date().toISOString().split('T')[0] }}",
        "templateContent": "={{ $('RAG Agent').item.json.output }}"
      },
      "id": "bayengage-template-create",
      "name": "BayEngage Create Template",
      "type": "n8n-nodes-bayengage.bayEngage",
