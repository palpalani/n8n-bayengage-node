        "bayEngageApi": {
          "id": "BAYENGAGE_API",
          "name": "BayEngage API"
        }
      }
    },
    {
      "parameters": {
        "resource": "campaign",
        "operation": "create",
        "templateId": "={{ $('BayEngage Create Template').item.json.id }}",
        "subject": "Daily Digest - {{ new Date().toLocaleDateString() }}",
        "contactId": "={{ $json.id }}"
      },
      "id": "bayengage-campaign-create",
