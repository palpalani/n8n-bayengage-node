        "resource": "template",
        "operation": "create",
        "templateName": "Product Launch - {{ $json.product_name || 'New Product' }}",
        "templateContent": "={{ $('RAG Agent').item.json.email_content || $('RAG Agent').item.json.output }}"
      },
      "id": "bayengage-template-create",
      "name": "BayEngage Create Template",
      "type": "n8n-nodes-bayengage.bayEngage",
      "typeVersion": 1,
