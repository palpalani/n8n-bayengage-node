      "credentials": {
        "bayEngageApi": {
          "id": "BAYENGAGE_API",
          "name": "BayEngage API"
        }
      }
    },
    {
      "parameters": {
        "resource": "event",
        "operation": "track",
        "eventName": "product_launch_email_sent",
        "contactId": "={{ $json.id }}",
        "eventProperties": {
          "values": [
            {
              "name": "product_name",
              "value": "={{ $json.product_name || 'Unknown' }}"
            },
            {
              "name": "campaign_id",
              "value": "={{ $('BayEngage Create Campaign').item.json.id }}"
            },
            {
              "name": "template_id",
              "value": "={{ $('BayEngage Create Template').item.json.id }}"
            }
          ]
        }
      },
      "id": "bayengage-event-track",
      "name": "BayEngage Track Event",
      "type": "n8n-nodes-bayengage.bayEngage",
      "typeVersion": 1,
      "position": [
        1450,
        -200
      ],
      "credentials": {
        "bayEngageApi": {
          "id": "BAYENGAGE_API",
          "name": "BayEngage API"
        }
      }
    },
    {
      "parameters": {
        "operation": "append",
        "documentId": {
          "__rl": true,
          "value": "SHEET_ID",
          "mode": "list",
          "cachedResultName": "Product Launch Email"
        },
        "sheetName": {
          "__rl": true,
          "value": "Log",
          "mode": "list",
          "cachedResultName": "Log"
        },
        "columns": {
          "mappingMode": "defineBelow",
          "value": "Status,Contact Email,Product Name,Subject Line,Template ID,Campaign ID,Send Status",
          "schema": [
            {
              "name": "Status",
              "value": "={{ $json.success ? 'Sent' : 'Failed' }}"
            },
            {
              "name": "Contact Email",
              "value": "={{ $json.email }}"
            },
            {
              "name": "Product Name",
              "value": "={{ $json.product_name || 'Unknown' }}"
            },
            {
              "name": "Subject Line",
              "value": "={{ $('RAG Agent').item.json.subject_line || 'Exciting New Product Launch!' }}"
            },
            {
              "name": "Template ID",
              "value": "={{ $('BayEngage Create Template').item.json.id }}"
            },
            {
              "name": "Campaign ID",
              "value": "={{ $('BayEngage Create Campaign').item.json.id }}"
            },
            {
              "name": "Send Status",
              "value": "={{ $('BayEngage Send Campaign').item.json.status || 'Pending' }}"
            }
          ]
        }
      },
