    {
      "parameters": {
        "promptType": "define",
        "text": "Handle data",
        "options": {
          "systemMessage": "You are an assistant for BayEngage Bounce Alert. Analyze email bounce data to determine bounce types (hard bounce, soft bounce, spam), identify patterns, and recommend appropriate actions. Return a JSON object with bounce analysis and recommended contact management actions."
        }
      },
      "id": "8c19f7ba-df58-44c6-8a70-70d747b0e2b9",
      "name": "RAG Agent",
      "type": "@n8n/n8n-nodes-langchain.agent",
      "typeVersion": 1,
      "position": [
        720,
        -40
      ]
    },
    {
      "parameters": {
        "resource": "contact",
        "operation": "get",
        "email": "={{ $json.email || $json.bounced_email || 'unknown@example.com' }}"
      },
      "id": "bayengage-contact-get",
      "name": "BayEngage Get Contact",
      "type": "n8n-nodes-bayengage.bayEngage",
      "typeVersion": 1,
      "position": [
        950,
        -40
      ],
      "credentials": {
        "bayEngageApi": {
          "id": "BAYENGAGE_API",
          "name": "BayEngage API"
        }
      }
    },
    {
      "parameters": {
        "resource": "contact",
        "operation": "update",
        "contactId": "={{ $json.id }}",
        "customFields": {
          "values": [
            {
              "name": "bounce_count",
              "value": "={{ ($json.bounce_count || 0) + 1 }}"
            },
            {
              "name": "last_bounce_date",
              "value": "={{ new Date().toISOString() }}"
            },
            {
              "name": "bounce_type",
              "value": "={{ $('RAG Agent').item.json.bounce_type || 'unknown' }}"
            },
            {
              "name": "bounce_reason",
              "value": "={{ $('RAG Agent').item.json.bounce_reason || 'Unknown' }}"
            }
          ]
        }
      },
      "id": "bayengage-contact-update",
      "name": "BayEngage Update Contact",
      "type": "n8n-nodes-bayengage.bayEngage",
      "typeVersion": 1,
      "position": [
        1200,
        -40
      ],
      "credentials": {
        "bayEngageApi": {
          "id": "BAYENGAGE_API",
          "name": "BayEngage API"
        }
      }
    },
    {
      "parameters": {
        "resource": "event",
        "operation": "track",
        "eventName": "email_bounced",
        "contactId": "={{ $('BayEngage Get Contact').item.json.id }}",
        "eventProperties": {
          "values": [
            {
              "name": "bounce_type",
              "value": "={{ $('RAG Agent').item.json.bounce_type || 'unknown' }}"
            },
            {
              "name": "bounce_reason",
              "value": "={{ $('RAG Agent').item.json.bounce_reason || 'Unknown' }}"
            },
            {
              "name": "campaign_id",
              "value": "={{ $json.campaign_id || 'N/A' }}"
            },
            {
              "name": "email_address",
              "value": "={{ $json.email || $json.bounced_email }}"
            }
          ]
        }
      },
      "id": "bayengage-event-track",
      "name": "BayEngage Track Event",
      "type": "n8n-nodes-bayengage.bayEngage",
      "typeVersion": 1,
      "position": [
        1200,
        -200
      ],
      "credentials": {
        "bayEngageApi": {
          "id": "BAYENGAGE_API",
          "name": "BayEngage API"
        }
      }
    },
    {
      "parameters": {
        "resource": "list",
        "operation": "removeContact",
        "listId": "={{ $('RAG Agent').item.json.list_id || 'active_subscribers' }}",
        "contactId": "={{ $('BayEngage Get Contact').item.json.id }}"
      },
      "id": "bayengage-list-remove",
      "name": "BayEngage Remove from List",
      "type": "n8n-nodes-bayengage.bayEngage",
      "typeVersion": 1,
      "position": [
        1450,
        -40
      ],
      "credentials": {
        "bayEngageApi": {
          "id": "BAYENGAGE_API",
          "name": "BayEngage API"
        }
      }
    },
    {
      "parameters": {
        "operation": "append",
        "documentId": {
          "__rl": true,
          "value": "SHEET_ID",
          "mode": "list",
          "cachedResultName": "BayEngage Bounce Alert"
        },
        "sheetName": {
          "__rl": true,
          "value": "Log",
          "mode": "list",
          "cachedResultName": "Log"
        },
        "columns": {
          "mappingMode": "defineBelow",
          "value": "Status,Email Address,Bounce Type,Bounce Reason,Contact ID,Action Taken",
          "schema": [
            {
              "name": "Status",
              "value": "={{ $json.success ? 'Processed' : 'Failed' }}"
            },
            {
              "name": "Email Address",
              "value": "={{ $json.email || $json.bounced_email }}"
            },
            {
              "name": "Bounce Type",
              "value": "={{ $('RAG Agent').item.json.bounce_type || 'unknown' }}"
            },
            {
              "name": "Bounce Reason",
              "value": "={{ $('RAG Agent').item.json.bounce_reason || 'Unknown' }}"
            },
            {
              "name": "Contact ID",
              "value": "={{ $('BayEngage Get Contact').item.json.id }}"
            },
            {
              "name": "Action Taken",
              "value": "={{ $('RAG Agent').item.json.recommended_action || 'Contact updated' }}"
            }
          ]
        }
      },
      "id": "adc64b6e-f045-4d6c-a38b-41c33af872f2",
      "name": "Append Sheet",
      "type": "n8n-nodes-base.googleSheets",
      "typeVersion": 4,
      "position": [
        1700,
        -40
      ],
      "credentials": {
        "googleSheetsOAuth2Api": {
          "id": "SHEETS_API",
          "name": "Google Sheets account"
        }
      }
    },
    {
      "parameters": {
        "channel": "#alerts",
        "text": "BayEngage Bounce Alert error: {{ $json.error.message }}"
      },
      "id": "dce8248e-a281-492c-bac7-fca5243ec8e5",
      "name": "Slack Alert",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1,
      "position": [
        1700,
        120
      ],
      "credentials": {
        "slackApi": {
          "id": "SLACK_API",
          "name": "Slack"
        }
      }
    }
  ],
